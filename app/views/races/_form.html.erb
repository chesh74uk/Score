<%= simple_form_for @race do |r| %>
    <%= r.input :heat_time %>
    <%= r.input :blue_score %>
    <%= r.input :red_score %>
    <%= r.input :white_score %>
    <%= r.input :yellow_score %>
    <%= r.hidden_field :heat_number %>
    <%= r.hidden_field :meeting_id %>


    <%= r.button :submit %>
<% end %>


